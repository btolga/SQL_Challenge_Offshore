# QUESTION 6: WHICH COMPANIES/INDIVIDUALS ARE INVOLVED THE MOST?

	SELECT Description_, COUNT(*) AS counts FROM nodesNW
	GROUP BY Description_
	ORDER BY counts DESC
	LIMIT 20;

